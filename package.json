  "dependencies": {
    "@react-google-maps/api": "^1.9.5",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongo-seeding": "^3.4.1",
    "mongoose": "^5.9.17",
    "morgan": "^1.10.0",
    "node-geocoder": "^3.27.0",
    "react-router-dom": "^5.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
